<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="800.0" prefWidth="999.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="income.view.JobOverviewController">
    <SplitPane dividerPositions="0.2" layoutX="188.0" layoutY="109.0" prefHeight="400.0" prefWidth="600.0"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                <TableView fx:id="jobsTable" prefHeight="398.0" prefWidth="100.0" AnchorPane.bottomAnchor="100.0"
                           AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
                    <columns>
                        <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Nazwa"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
                <Label alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="50.0"
                       prefWidth="150.0" text="Praca" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"
                       AnchorPane.topAnchor="0.0">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
                <Button layoutX="52.0" layoutY="348.0" mnemonicParsing="false" onAction="#handleEditJob"
                        text="Edytuj prace" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0"
                        AnchorPane.rightAnchor="0.0"/>
                <Button layoutX="108.0" layoutY="508.0" mnemonicParsing="false" onAction="#handleNewJob"
                        text="Dodaj prace" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0"
                        AnchorPane.rightAnchor="0.0"/>
            </AnchorPane>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="50.0" prefWidth="150.0" text="Informacje"
                       AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="0.0">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
                <TabPane layoutX="64.0" layoutY="47.0" prefHeight="398.0" prefWidth="407.0"
                         tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
                    <Tab text="Ogólne">
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <GridPane layoutX="-6.0" prefHeight="225.0" prefWidth="407.0" AnchorPane.leftAnchor="10.0"
                                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="263.0" minWidth="10.0"
                                                       prefWidth="193.0"/>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="342.0" minWidth="10.0"
                                                       prefWidth="342.0"/>
                                </columnConstraints>
                                <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                </rowConstraints>
                                    <Label text="Roczny dochód"/>
                                    <Label text="Miesięczny dochód" GridPane.rowIndex="1"/>
                                    <Label text="Ilość godzin w roku" GridPane.rowIndex="2"/>
                                    <Label text="Ilość godzin w miesiącu" GridPane.rowIndex="3"/>
                                    <Label fx:id="yearIncome" text="Label" GridPane.columnIndex="1"/>
                                    <Label fx:id="monthIncome" text="Label" GridPane.columnIndex="1"
                                           GridPane.rowIndex="1"/>
                                    <Label fx:id="hoursInYear" text="Label" GridPane.columnIndex="1"
                                           GridPane.rowIndex="2"/>
                                    <Label fx:id="hoursInMonth" text="Label" GridPane.columnIndex="1"
                                           GridPane.rowIndex="3"/>
                            </GridPane>
                        </AnchorPane>
                    </Tab>
                    <Tab text="Szczegółowe">
                            <AnchorPane minHeight="0.0" minWidth="0.0">
                                    <TableView fx:id="jobsDetailsTable" layoutX="104.0" layoutY="52.0"
                                               prefHeight="500.0" prefWidth="654.0" AnchorPane.bottomAnchor="60.0"
                                               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                               AnchorPane.topAnchor="0.0">
                                        <columns>
                                            <TableColumn fx:id="dateColumn" prefWidth="75.0" text="Data"/>
                                            <TableColumn fx:id="hourColumn" prefWidth="75.0" text="Godziny"/>
                                            <TableColumn fx:id="incomeColumn" prefWidth="75.0" text="Stawka"/>
                                        </columns>
                                        <columnResizePolicy>
                                            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                        </columnResizePolicy>
                                    </TableView>
                                    <HBox prefHeight="0.0" prefWidth="654.0" spacing="20.0"
                                          AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
                                          AnchorPane.rightAnchor="10.0">
                                            <Button mnemonicParsing="false" text="Zestawienie miesięcy"/>
                                            <Button mnemonicParsing="false" text="Dodaj zarobek"/>
                                            <Button mnemonicParsing="false" text="Dodaj zarobki"/>
                                        <opaqueInsets>
                                            <Insets/>
                                        </opaqueInsets>
                                    </HBox>
                            </AnchorPane>
                    </Tab>
                </TabPane>
            </AnchorPane>
    </SplitPane>
</AnchorPane>
